{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "maple",
  "name": "ScentedHub",
  "colors": {
    "primary": "#16A34A",
    "light": "#07C983",
    "dark": "#15803D"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "groups": [
      {
        "group": "Get Started",
        "pages": ["introduction", "quickstart", "development"]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Documentation",
          "href": "https://mintlify.com/docs",
          "icon": "book-open-cover"
